<p id="notice"><%= notice %></p>

<h1>Candidates</h1>

<table>
  <thead>
    <tr>
      <th>Fullname</th>
      <th>Email</th>
      <th>Cellphone</th>
      <th>Date birth</th>
      <th>Gender</th>
      <th>Job</th>
      <th>File</th>
      <th>Linkedin</th>
      <th>Portfolio</th>
      <th>Short description</th>
      <th>Accepted</th>
      <th>Available change city</th>
      <th>License car</th>
      <th>Type license car</th>
      <th>Have car</th>
      <th>City</th>
      <th>State</th>
      <th>Country</th>
      <th>Slug</th>
      <th>Good profile</th>
      <th>Observation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @candidates.each do |candidate| %>
      <tr>
        <td><%= candidate.fullname %></td>
        <td><%= candidate.email %></td>
        <td><%= candidate.cellphone %></td>
        <td><%= candidate.date_birth %></td>
        <td><%= candidate.gender %></td>
        <td><%= candidate.job %></td>
        <td><%= candidate.file %></td>
        <td><%= candidate.linkedin %></td>
        <td><%= candidate.portfolio %></td>
        <td><%= candidate.short_description %></td>
        <td><%= candidate.accepted %></td>
        <td><%= candidate.available_change_city %></td>
        <td><%= candidate.license_car %></td>
        <td><%= candidate.type_license_car %></td>
        <td><%= candidate.have_car %></td>
        <td><%= candidate.city %></td>
        <td><%= candidate.state %></td>
        <td><%= candidate.country %></td>
        <td><%= candidate.slug %></td>
        <td><%= candidate.good_profile %></td>
        <td><%= candidate.observation %></td>
        <td><%= link_to 'Show', candidate %></td>
        <td><%= link_to 'Edit', edit_candidate_path(candidate) %></td>
        <td><%= link_to 'Destroy', candidate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Candidate', new_candidate_path %>
